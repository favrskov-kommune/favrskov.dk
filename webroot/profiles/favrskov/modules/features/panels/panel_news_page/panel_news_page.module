<?php
/**
 * @file
 * Code for the Panel News page feature.
 */

include_once 'panel_news_page.features.inc';

/**
 * Implementation of hook_ctools_plugin_directory().
 */
function panel_news_page_ctools_plugin_directory($module, $plugin) {
  if ($module == 'ctools' && !empty($plugin)) {
    return 'plugins/' . $plugin;
  }
}