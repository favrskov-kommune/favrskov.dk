include_once 'panel_footer_mini_panel.features.inc';

/**
 * Implements hook_ctools_plugin_directory().
 */
function panel_footer_mini_panel_ctools_plugin_directory($owner, $plugin_type) {
  if ($owner == 'ctools' && $plugin_type == 'content_types') {
    return 'plugins/content_types';
  }
}
